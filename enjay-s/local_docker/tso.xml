<?xml version="1.0" ?>
<yandex>
    <logger>
        <level>trace</level>
        <log>/data1/songenjie001/deploy/byconity/logs/tso.log</log>
        <errorlog>/data1/songenjie001/deploy/byconity/logs/tso.err.log</errorlog>
        <size>1000M</size>
        <count>10</count>
    </logger>
    <tso_service>
        <type>fdb</type>
        <port>18689</port> <!-- 建议修改为rpc_port-->
        <fdb>
            <cluster_file>/data1/songenjie001/deploy/byconity/config/fdb/fdb.cluster</cluster_file>
        </fdb>
        <tso_window_ms>3000</tso_window_ms>
        <tso_get_leader_info_interval_ms>0</tso_get_leader_info_interval_ms>
        <tso_max_retry_count>3</tso_max_retry_count>
    </tso_service>
    <cnch_config>/data1/songenjie001/deploy/byconity/config/cnch_config.xml</cnch_config>
</yandex>

